{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Archive Page",
  "author": "John Navas",
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "optional_permissions": [ "notifications" ],
